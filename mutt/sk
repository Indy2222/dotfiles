set smtp_pass=`pass show sk/mail-pwd | head -n 1 | tr -d '\n'`
set smtp_url="smtps://indra@spaceknow.com@smtp.gmail.com:465"
set smtp_authenticators="plain"

set from="indra@spaceknow.com"
set folder="~/mail/sk"
set spoolfile="+INBOX"
set record="+Sent"
set postponed="+Drafts"
set header_cache="~/.mutt/sk_header_cache"
set signature="~/.mutt/sk_signature.txt"

unmailboxes *
mailboxes =INBOX =Alerts =Gerrit =Sent =Drafts
